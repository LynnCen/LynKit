{
  "name": "@lynkit/ui",
  "version": "0.3.0",
  "description": "React UI Component Library",
  "author": "LynnCen",
  "homepage": "https://github.com/LynnCen/LynKit#readme",
  "license": "MIT",
  "type": "module",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.js"
    },
    "./es/*": "./es/*",
    "./lib/*": "./lib/*",
    "./styles.css": "./es/style.css"
  },
  "files": [
    "es",
    "lib"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LynnCen/LynKit.git"
  },
  "bugs": {
    "url": "https://github.com/LynnCen/LynKit/issues"
  },
  "dependencies": {
    "classnames": "catalog:"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@vitejs/plugin-react": "^4.3.4",
    "jsdom": "^26.0.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.4.0",
    "vitest": "^2.1.8",
    "typescript": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "less": "^4.2.0"
  }
}
