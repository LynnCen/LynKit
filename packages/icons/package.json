{
  "name": "@lynkit/icons",
  "version": "0.2.2",
  "description": "Icon components for Lynkit",
  "author": "LynnCen",
  "homepage": "https://github.com/LynnCen/LynKit#readme",
  "license": "MIT",
  "type": "module",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.js"
    },
    "./es/*": "./es/*",
    "./lib/*": "./lib/*"
  },
  "files": [
    "es",
    "lib",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "transform": "node scripts/transform-icons.mjs",
    "build": "vite build",
    "dev": "vite build --watch"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LynnCen/LynKit.git"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.4.0",
    "typescript": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:"
  }
}
