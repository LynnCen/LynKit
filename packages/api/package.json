{
  "name": "@lynkit/api",
  "version": "0.3.0",
  "description": "Utility functions for Lynkit",
  "author": "LynnCen",
  "license": "MIT",
  "type": "module",
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./*": {
      "types": "./es/*/index.d.ts",
      "import": "./es/*/index.js",
      "require": "./lib/*/index.cjs"
    }
  },
  "files": [
    "es",
    "lib"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LynnCen/LynKit.git"
  },
  "keywords": [
    "utils",
    "debounce",
    "throttle",
    "lru-cache",
    "retry"
  ],
  "devDependencies": {
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
