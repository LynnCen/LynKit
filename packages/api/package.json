{
  "name": "@lynkit/api",
  "version": "0.2.3",
  "description": "Utility functions for Lynkit",
  "author": "LynnCen",
  "license": "MIT",
  "type": "module",
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "files": [
    "lib"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LynnCen/LynKit.git"
  },
  "keywords": [
    "utils",
    "api",
    "functions"
  ],
  "devDependencies": {
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
}
