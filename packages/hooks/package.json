{
  "name": "@lynkit/hooks",
  "version": "0.2.2",
  "description": "React Hooks Library",
  "author": "LynnCen",
  "homepage": "https://github.com/LynnCen/LynKit#readme",
  "license": "MIT",
  "type": "module",
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./*": {
      "types": "./es/*/index.d.ts",
      "import": "./es/*/index.js",
      "require": "./lib/*/index.cjs"
    }
  },
  "files": [
    "es",
    "lib"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LynnCen/LynKit.git"
  },
  "bugs": {
    "url": "https://github.com/LynnCen/LynKit/issues"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "typescript": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "vitest": "catalog:",
    "@testing-library/react": "catalog:"
  }
}
